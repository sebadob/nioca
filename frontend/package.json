		"@sveltejs/kit": "^2.8.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte": "^2.32.4",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.19",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^5.4.6",
		"yup": "^1.2.0"
	},
	"type": "module"
